<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SeedActivity">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="325dp"
        android:layout_height="58dp"
        android:text="Zapisz wygenerowane seedy \n BedÄ… podatne przy odzyskiwaniu dostepu do aplikacji"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/seed_output"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.604"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.62" />

    <TextView
        android:id="@+id/seed_output"
        android:layout_width="301dp"
        android:layout_height="423dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_seed_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="openWalletLayout"
        android:text="OK, potwierdam"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seed_output" />


</androidx.constraintlayout.widget.ConstraintLayout>